#!/usr/bin/env ruby
# frozen_string_literal: true

require_relative '../lib/keep_warm'

if ARGV.length != 1
  puts "Usage: #{$PROGRAM_NAME} file_name.txt"
  exit 1
end

KeepWarm.configure do |config|
  config.format = :json
  config.output = :file
end

filename = ARGV[0]
KeepWarm::Processor.new(filename).run
